#include <bits/stdc++.h>
using namespace std;
#define MAXN 1000000
int N, x, y, x2, y2;

// sweep line algorithm

struct Point {
	int x, y, e;
	// e = 0: horizontal enter
	// e = 1: leaving
	// e = 2: vertical enter
	Point (int x, int y, int e) {
		this->x = x;
		this->y = y;
		this->e = e;
	}
};

int vPoints[MAXN], segtree[MAXN*2+1];
vector<Point> points;
set<int> y_values;
vector<int> y_map(MAXN);

int main() {
	scanf("%d", &N);
	for (int i = 0; i < N; i++) {
		scanf("%d %d %d %d", &x, &y, &x2, &y2);
		y_values.insert(y);
		y_values.insert(y2);
		if (x == x2) {
			points.push_back(Point(x, y, 0));
			points.push_back(Point(x2, y2, 1));	
		} else if (y == y2) {
			points.push_back(Point(x, y, 2));
			points.push_back(Point(x2, y2, 1));
		}
	}
	int k = 0;
	for (int x : y_values) {
		y_map[y_values[k]] = k;
		k++;
	}
	

}
